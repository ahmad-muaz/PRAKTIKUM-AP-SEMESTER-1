#include <iostream>

using namespace std;

int main() {
    char a;
    int b;
    cout <<"Konversi bilangan bulat positif ke dalam angka Romawi : "<<endl;
    cout <<"Ketikkan angka : ";
    cin >> b;
    switch(b) {
        case 1 :
        b = 1;
        cout<<"I";
        break;
        case 2 :
        b = 2;
        cout<<"II";
        break;
        case 3 :
        b = 3;
        cout<<"III";
        break;
        case 4 :
        b = 4;
        cout<<"IV";
        break;
        case 5 :
        b = 5;
        cout<<"V";
        break;
        case 9 :
        b = 9;
        cout<<"IX";
        break;
        case 10 :
        b = 10;
        cout<<"X";
        break;
        case 20 :
        b = 20;
        cout<<"XX";
        break;
        case 30 :
        b = 30;
        cout<<"XXX";
        break;
        case 40 :
        b = 40;
        cout<<"XL";
        break;
        case 50 :
        b = 50;
        cout<<"L";
        break;
        case 90 :
        b = 90;
        cout<<"XC";
        break;
        case 100 :
        b = 100;
        cout<<"XC";
        break;
        case 200 :
        b = 200;
        cout<<"CC";
        break;
        case 300 :
        b = 300;
        cout<<"CCC";
        break;
        case 400 :
        b = 400;
        cout<<"CD";
        break;
        case 500 :
        b = 500;
        cout<<"D";
        break;
        case 900 :
        b = 900;
        cout<<"CM";
        break;
        case 1000 :
        b = 1000;
        cout<<"M";
        break;
        case 2000 :
        b = 2000;
        cout<<"MM";
        break;
        case 3000 :
        b = 3000;
        cout<<"MMM";
        break;
    }
    return 0;
}
